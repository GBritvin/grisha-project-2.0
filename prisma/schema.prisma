generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      =  "postgresql://postgres:Gfifregk11@localhost:5432/mydb?schema=public"
}
model User {
  id   Int    @id @default(autoincrement())
  name String
}

model Pokemon {
  id             Int      @id @default(autoincrement())
  name           String
  weight         Int
  height         Int
  baseExperience Int
  species        String
  experience     Int
}

